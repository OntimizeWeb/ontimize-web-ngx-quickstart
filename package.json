{
  "name": "ontimize-web-ngx-quickstart",
  "version": "8.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4800",
    "test": "ng test --code-coverage",
    "test-ci": "ng test --no-watch --code-coverage --browsers ChromeHeadless",
    "lint": "ng lint",
    "build": "ng build",
    "production": "ng build --base-href /ontimize-web-ngx-quickstart",
    "production-aot-server": "ng build --aot --base-href /ontimizeweb/v8/quickstart/",
    "production-aot-serverpwa": "ng build --aot --base-href /ontimizeweb/v8/quickstart/ && npm run build-ngsw",
    "development-aot-serverpwa": "ng build --aot --base-href / && npm run build-ngsw",
    "build-ngsw": "npm run build-ngsw-config",
    "build-ngsw-config": "node_modules/.bin/ngsw-config dist ngsw-config.json ontimize-web-ngx-quickstart-pwa",
    "build:themes": "npx node-sass ./src/assets/themes/ -o ./src/assets/themes/ --recursive"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "~8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/flex-layout": "~8.0.0-beta.27",
    "@angular/forms": "~8.2.14",
    "@angular/material": "~8.2.3",
    "@angular/material-moment-adapter": "^8.2.3",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/platform-server": "~8.2.14",
    "@angular/pwa": "^0.803.29",
    "@angular/router": "~8.2.14",
    "@angular/upgrade": "~8.2.14",
    "hammerjs": "^2.0.8",
    "ontimize-web-ngx": "8.8.0",
    "ontimize-web-ngx-report": "8.0.2",
    "ontimize-web-ngx-charts": "8.0.1",
    "ontimize-web-ngx-filemanager": "8.5.5",
    "ontimize-web-ngx-map": "8.0.1",
    "ontimize-web-ngx-theming": "8.4.0",
    "rxjs": "~6.4.0",
    "tslib": "~1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "~8.3.29",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@angular/service-worker": "^8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-sonarqube-unit-reporter": "0.0.23",
    "ng-packagr": "^5.4.0",
    "protractor": "~7.0.0",
    "sonarqube-scanner": "^2.8.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3",
    "socket.io": ">=2.4.0",
    "ini": ">=1.3.6",
    "webpack-subresource-integrity": ">=1.5.1"
  }
