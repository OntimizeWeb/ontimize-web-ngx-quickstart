{
  "name": "ontimize-web-ng2-quickstart",
  "version": "2.0.0-rc.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "aot": "npm run clean:all && npm run aot:compile && npm run aot:bundle && npm run styles",
    "aot:bundle": "webpack --config config/webpack-aot.config.js --bail",
    "aot:compile": "node_modules/.bin/ngc -p tsconfig.aot.json",
    "start": "npm run clean:all && npm run aot:compile && ng serve --port=4209",
    "clean": "rimraf dist",
    "clean:aot": " rimraf src/node_modules && rimraf src/app/**/*.ngfactory.ts && rimraf src/app/**/*.ngsummary.json && rimraf src/app/**/*.ngstyle.ts && rimraf src/app/**/*.d.ts",
    "clean:all": "npm run clean && npm run clean:aot",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "production": "npm run clean && ng build --aot=false --target -prod --base-href /ontimize-web-ng2-quickstart/",
    "styles": "node-sass src/styles-aot.scss dist/assets/css/app.css --output-style compressed"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.3.6",
    "@angular/cdk": "2.0.0-beta.10",
    "@angular/common": "4.3.6",
    "@angular/compiler": "4.3.6",
    "@angular/core": "4.3.6",
    "@angular/flex-layout": "2.0.0-beta.9",
    "@angular/forms": "4.3.6",
    "@angular/http": "4.3.6",
    "@angular/material": "2.0.0-beta.10",
    "@angular/platform-browser": "4.3.6",
    "@angular/platform-browser-dynamic": "4.3.6",
    "@angular/platform-server": "4.3.6",
    "@angular/router": "4.3.6",
    "@angular/upgrade": "4.3.6",
    "@ngx-translate/core": "8.0.0",
    "@ngx-translate/http-loader": "1.0.2",
    "core-js": "^2.4.1",
    "lodash": "^4.17.4",
    "ontimize-web-ng2": "2.0.0-rc.8",
    "ontimize-web-ng2-charts": "2.0.0-rc.3",
    "ontimize-web-ngx-datatable": "1.0.0",
    "reflect-metadata": "0.1.10",
    "rxjs": "^5.4.2",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@angular/cli": "1.3.2",
    "@angular/compiler-cli": "4.3.6",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "3.1.1",
    "flag-icon-css": "^2.8.0",
    "hammerjs": "2.0.8",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "5.2.0",
    "typescript": "2.3.4"
  }
}