{
  "name": "ontimize-web-ngx-quickstart",
  "version": "8.4.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4800",
    "test": "ng test --code-coverage",
    "test-ci": "ng test --no-watch --code-coverage --browsers ChromeHeadless",
    "lint": "ng lint",
    "build": "ng build",
    "production": "ng build --base-href /ontimize-web-ngx-quickstart",
    "production-aot-server": "ng build --configuration production --base-href /ontimizeweb/v8/quickstart/",
    "production-aot-serverpwa": "ng build --configuration production --base-href /ontimizeweb/v8/quickstart/ && npm run build-ngsw",
    "development-aot-serverpwa": "ng build --configuration production --base-href / && npm run build-ngsw",
    "build-ngsw": "npm run build-ngsw-config",
    "build-ngsw-config": "node_modules/.bin/ngsw-config dist ngsw-config.json ontimize-web-ngx-quickstart-pwa",
    "build:themes": "npx node-sass ./src/assets/themes/ -o ./src/assets/themes/ --recursive"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^13.3.9",
    "@angular/material-moment-adapter": "^13.3.9",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/platform-server": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@angular/upgrade": "^14.3.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.12",
    "@angular-devkit/build-optimizer": "^0.803.23",
    "@angular/cli": "^14.2.12",
    "@angular/compiler-cli": "^14.3.0",
    "@angular/language-service": "^14.3.0",
    "@angular/service-worker": "^14.3.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "codelyzer": "^6.0.0",
    "ini": ">=1.3.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-sass": "^4.14.1",
    "socket.io": ">=2.4.0",
    "karma-sonarqube-unit-reporter": "0.0.23",
    "ng-packagr": "^14.2.2",
    "protractor": "~7.0.0",
    "sonarqube-scanner": "^2.8.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4",
    "webpack-subresource-integrity": ">=1.5.1"
  }
}
