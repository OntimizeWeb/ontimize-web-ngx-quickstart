{
  "name": "ontimize-web-ng2-quickstart",
  "version": "2.0.0-rc.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4204",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf dist",
    "production": "npm run clean && ng build --aot=false --target -prod --base-href /ontimize-web-ng2-quickstart/",
    "ngComp": "npm run component && npm run moduleR",
    "component": "ng generate component --skip-import=true --spec=false process.env.component || 'empty'",
    "module": "ng generate module main/customers/",
    "moduleR": "ng generate module --routing  process.env.component || 'empty'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.2.4",
