{
  "name": "ontimize-web-ngx-quickstart",
  "version": "4.0.1",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "npm run build-themes && ng serve --port 4400",
    "build": "npm run build-themes && ng build",
    "production": "npm run build-themes && ng build --aot=false --target -prod --base-href /ontimize-web-ngx-quickstart/",
    "production-aot": "npm run build-themes && ontimize-web-ngx production-aot --project-name ontimize-web-ngx-quickstart --href /ontimize-web-ngx-quickstart/",
    "production-aot-server": "npm run build-themes && ontimize-web-ngx production-aot --project-name ontimize-web-ngx-quickstart --href /ontimizeweb/quickstart/",
    "production-aot-serverpwa": "npm run build-themes && ontimize-web-ngx production-aot --project-name ontimize-web-ngx-quickstart --href /ontimizeweb/quickstartpwa/ && npm run build-ngsw",
    "build-themes": "node-sass ./src/assets/themes/ -o ./src/assets/themes/ --recursive",
    "build-ngsw": "npm run build-ngsw-config && node cp-ngsw-dist.js",
    "build-ngsw-config": "node_modules/.bin/ngsw-config dist src/ngsw-config.json /ontimizeweb/quickstartpwa/ "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.10",
    "@angular/cdk": "6.4.7",
    "@angular/common": "6.1.10",
    "@angular/compiler": "6.1.10",
    "@angular/core": "6.1.10",
    "@angular/flex-layout": "6.0.0-beta.18",
    "@angular/forms": "6.1.10",
    "@angular/http": "6.1.10",
    "@angular/material": "6.4.7",
    "@angular/material-moment-adapter": "6.4.7",
    "@angular/platform-browser": "6.1.10",
    "@angular/platform-browser-dynamic": "6.1.10",
    "@angular/platform-server": "6.1.10",
    "@angular/pwa": "0.6.8",
    "@angular/router": "6.1.10",
    "@angular/service-worker": "6.1.10",
    "@angular/upgrade": "6.1.10",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "2.5.7",
    "ontimize-web-ngx": "4.0.7",
    "ontimize-web-ngx-charts": "4.0.4",
    "ontimize-web-ngx-filemanager": "4.0.1",
    "ontimize-web-ngx-map": "4.0.1",
    "ontimize-web-ngx-theming": "4.0.2",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.3.3",
    "web-animations-js": "^2.3.1",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular/cli": "6.2.6",
    "@angular/compiler-cli": "6.1.10",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "compression-webpack-plugin": "1.1.12",
    "copy-webpack-plugin": "4.5.4",
    "html-webpack-plugin": "4.0.0-beta.2",
    "node-sass": "4.9.4",
    "ontimize-web-ngx-tools": "4.0.0",
    "ts-node": "~2.0.0",
    "typescript": "2.9.2",
    "webpack": "4.18.0",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": ">=3.1.11",
    "webpack-pwa-manifest": "4.0.0",
    "serialize-javascript": ">=2.1.1"
  }
}
