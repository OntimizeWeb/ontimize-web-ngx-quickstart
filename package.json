    "@angular/compiler-cli": "4.2.4",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "3.1.1",
    "flag-icon-css": "^2.8.0",
    "hammerjs": "2.0.8",
