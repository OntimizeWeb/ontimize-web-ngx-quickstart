{
  "name": "ontimize-web-ngx-quickstart",
  "version": "8.4.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4800",
    "test": "ng test --code-coverage",
    "test-ci": "ng test --no-watch --code-coverage --browsers ChromeHeadless",
    "lint": "ng lint",
    "build": "ng build",
    "production": "ng build --base-href /ontimize-web-ngx-quickstart",
    "production-aot-server": "ng build --configuration production --base-href /ontimizeweb/v8/quickstart/",
    "production-aot-serverpwa": "ng build --configuration production --base-href /ontimizeweb/v8/quickstart/ && npm run build-ngsw",
    "development-aot-serverpwa": "ng build --configuration production --base-href / && npm run build-ngsw",
    "build-ngsw": "npm run build-ngsw-config",
    "build-ngsw-config": "node_modules/.bin/ngsw-config dist ngsw-config.json ontimize-web-ngx-quickstart-pwa",
    "build:themes": "npx node-sass ./src/assets/themes/ -o ./src/assets/themes/ --recursive"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.17",
    "@angular/cdk": "~11.2.13",
    "@angular/common": "~12.2.17",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~12.2.17",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "~12.2.17",
    "@angular/material": "~11.2.13",
    "@angular/material-moment-adapter": "^11.2.13",
    "@angular/platform-browser": "~12.2.17",
    "@angular/platform-browser-dynamic": "~12.2.17",
    "@angular/platform-server": "~12.2.17",
    "@angular/router": "~12.2.17",
    "@angular/upgrade": "~12.2.17",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular-devkit/build-optimizer": "~0.803.23",
    "@angular/cli": "~12.2.18",
    "@angular/compiler-cli": "~12.2.17",
    "@angular/language-service": "~12.2.17",
    "@angular/service-worker": "^12.2.17",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "ini": ">=1.3.6",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-sass": "^4.14.1",
    "socket.io": ">=2.4.0",
    "karma-sonarqube-unit-reporter": "0.0.23",
    "ng-packagr": "^12.2.7",
    "protractor": "~7.0.0",
    "sonarqube-scanner": "^2.8.1",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5",
    "webpack-subresource-integrity": ">=1.5.1"
  }
}
